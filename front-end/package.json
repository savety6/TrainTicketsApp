{
  "name": "front-end",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest src"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^5.0.1",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "svelte": "^4.2.12",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "babel-jest": "^29.7.0",
    "jest-transform-svelte": "^2.1.1",
    "svelte-jester": "^3.0.0",
    "svelte-routing": "^2.12.0"
  },
  "jest": {
    "bail": false,
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "svelte"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.tsx$": "ts-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "verbose": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
